{
  "name": "shopykart",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "preview": "vite preview",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "ts-node-dev --respawn --project tsconfig.server.json server",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "tsc && vite build",
    "build": "npm run build:server && npm run build:client",
    "start": "node dist/server"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "bcrypt": "^5.0.1",
    "bootstrap": "^5.1.3",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "firebase": "^9.8.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.6",
    "react": "^18.0.0",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^18.0.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.41",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "concurrently": "^7.2.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vitest": "^0.16.0"
  }
}
